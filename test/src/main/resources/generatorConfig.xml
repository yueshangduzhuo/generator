<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
	<!-- 引入配置文件 -->
	<properties resource="jdbc.properties" />

	<context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">
		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />
		<!-- 生成的文件编码 -->
		<property name="javaFileEncoding" value="utf-8"/>
		<!-- 通过自定义插件类生成自定义注解和接口 -->
		<!--<plugin type="com.zll.common.generator.GenPlugin">
			<property name="mappers" value="com.zll.mapper.BaseMapper" />
		</plugin>-->

		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />

		<commentGenerator>
			<!-- 取消生成注释 -->
			<property name="suppressAllComments" value="true"/>
		</commentGenerator>
		<!-- 数据库连接属性 -->
		<jdbcConnection driverClass="${jdbc.driver}" connectionURL="${jdbc.url}" userId="${jdbc.username}" password="${jdbc.password}" />
		<!-- 生成实体类配置 -->
		<javaModelGenerator targetPackage="com.zll.model" targetProject="src/main/java" />
		<!-- 生成映射文件配置 -->
		<sqlMapGenerator targetPackage="mapper" targetProject="src/main/resources" />
		<!-- 生成映射接口配置 -->
		<javaClientGenerator targetPackage="com.zll.mapper" targetProject="src/main/java" type="XMLMAPPER" />
		
		<table tableName="%">
			<!--mysql 配置 -->
			<generatedKey column="id" sqlStatement="Mysql" identity="true" />
		</table>
	</context>
</generatorConfiguration>